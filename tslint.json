{
    "extends": "tslint:recommended",
    "defaultSeverity": "error",
    "rules":{
        "no-console":{
            "severity": "warning",
            "options": [
                "debug",
                "info",
                "log",
                "error",
                "time",
                "timeEnd",
                "trace"
            ]
        }
    },
    "jsRules": {}
}